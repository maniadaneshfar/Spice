
@model Spice.Models.Category
@{
    ViewData["Titel"] = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<br />
<h2 class="text-info">Delete Category</h2>
<br />
<form method="post" asp-action="Delete">
    <div class="backgroundWhite border">
        
        <div class="form-group row">
            <div class="col-2">
                <lable asp-for="Name" class="col-form-label"></lable>
            </div>
            <div class="col-5">
                <input asp-for="@Model.Name" readonly class="form-control" />
            </div>
          

        </div>
        <div class="form-group row">
            <div class="offset-2 col-5">
                <partial name="_DeleteAndBackToListButton" model="@Model.Id" />
            </div>

        </div>
    </div>
</form>
