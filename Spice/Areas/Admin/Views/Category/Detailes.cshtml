@model Spice.Models.Category

@{ ViewData["Title"] = "Detailes"; }

<br />
<h2 class="text-info">Category Detailes</h2>
<br />
<div class=" border  backgroundWhite">
    <div class="form-group row">
        <div class="col-2">
            <label asp-for="Name"></label>
        </div>
        <div class="col-5">
            <input asp-for="@Model.Name" disabled class="form-control" />
        </div>
    </div>

    <div class="form-group row">
        <div class="col-5 offset-2">
            <partial name="_DetailesAndBackToListButton" model="@Model.Id" />
        </div>
    </div>

</div>
